{
	"name": "otomad-helper",
	"displayName": "Otomad Helper",
	"version": "7.9.0",
	"description": "The front-end part of Otomad Helper extension.",
	"type": "module",
	"main": "./dist/index.html",
	"module": "esnext",
	"repository": {
		"type": "git",
		"url": "https://github.com/otomad/OtomadHelper.git"
	},
	"homepage": "https://otomadhelper.readthedocs.io/",
	"bugs": "https://github.com/otomad/OtomadHelper/issues",
	"author": "Ranne",
	"license": "GPL-3.0-only",
	"scripts": {
		"dev": "vite --host",
		"dev-force": "vite --host --force",
		"dev-delete-auto-types": "pnpm run delete-auto-types && vite --host --force",
		"build": "vite build",
		"preview": "vite preview",
		"prebuild": "scripts\\prebuild.bat",
		"delete-auto-types": "rimraf -g src/types/auto-*.d.ts",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"lint:css": "stylelint './src/**/*.{js,jsx,ts,tsx}'",
		"release": "pnpm run build && gh release create $(node -pe \"require('./package.json').version\") $(node -pe \"require('./package.json').main\")",
		"compact-svg": "tsx scripts/compact-svg.ts",
		"crowdin:update": "tsx scripts/crowdin/update.ts",
		"crowdin:download": "tsx scripts/crowdin/download.ts",
		"commit": "git-cz"
	},
	"dependencies": {
		"@dnd-kit/core": "^6.1.0",
		"@dnd-kit/sortable": "^8.0.0",
		"ani-cursor": "^0.0.5",
		"classnames": "^2.5.1",
		"cz-conventional-changelog-zh": "^0.0.2",
		"i18next": "^23.11.1",
		"i18next-browser-languagedetector": "^7.2.1",
		"immer": "^10.0.4",
		"lodash-es": "^4.17.21",
		"lottie-web": "^5.12.2",
		"mitt": "^3.0.1",
		"pangu": "^4.0.7",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-flip-move": "^3.0.5",
		"react-i18next": "^13.5.0",
		"react-transition-group": "file:/react-transition-group/",
		"styled-components": "^6.1.8",
		"styled-tools": "^1.7.2",
		"use-immer": "^0.9.0",
		"valtio": "^1.13.2"
	},
	"devDependencies": {
		"@crowdin/crowdin-api-client": "^1.33.1",
		"@styled/typescript-styled-plugin": "^1.0.1",
		"@stylistic/eslint-plugin": "^1.7.0",
		"@stylistic/stylelint-plugin": "^2.1.1",
		"@types/lodash-es": "^4.17.12",
		"@types/pangu": "^4.0.2",
		"@types/react": "^18.2.78",
		"@types/react-dom": "^18.2.25",
		"@types/react-transition-group": "^4.4.10",
		"@vitejs/plugin-react": "^4.2.1",
		"babel-plugin-styled-components": "^2.1.4",
		"babel-plugin-transform-jsx-classnames": "^1.2.2",
		"click-to-react-component": "^1.1.0",
		"eslint": "^8.57.0",
		"eslint-plugin-react": "^7.34.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.6",
		"extract-zip": "^2.0.1",
		"lightningcss": "^1.24.1",
		"postcss-styled-syntax": "^0.6.4",
		"stylelint": "^16.3.1",
		"stylelint-config-standard": "^36.0.0",
		"stylelint-order": "^6.0.4",
		"terser": "^5.30.3",
		"typescript": "^5.4.5",
		"typescript-eslint": "^7.6.0",
		"unplugin-auto-import": "^0.17.5",
		"vite": "^5.2.8",
		"vite-plugin-glsl": "^1.3.0",
		"vite-plugin-html-minifier": "^1.0.4",
		"vite-plugin-svg-icons": "^2.0.1",
		"vite-plugin-svgr": "^4.2.0",
		"vite-tsconfig-paths": "^4.3.2",
		"xmlbuilder2": "^3.1.1"
	},
	"resolutions": {
		"postcss": "^8.4.38"
	},
	"pnpm": {
		"patchedDependencies": {
			"babel-plugin-styled-components@2.1.4": "patches/babel-plugin-styled-components@2.1.4.patch",
			"babel-plugin-transform-jsx-classnames@1.2.2": "patches/babel-plugin-transform-jsx-classnames@1.2.2.patch",
			"lottie-web@5.12.2": "patches/lottie-web@5.12.2.patch",
			"pangu@4.0.7": "patches/pangu@4.0.7.patch",
			"postcss-styled-syntax@0.6.4": "patches/postcss-styled-syntax@0.6.4.patch",
			"react@18.2.0": "patches/react@18.2.0.patch",
			"react-dom@18.2.0": "patches/react-dom@18.2.0.patch",
			"react-flip-move@3.0.5": "patches/react-flip-move@3.0.5.patch",
			"@types/react@18.2.78": "patches/@types__react@18.2.78.patch",
			"postcss-selector-parser@6.0.16": "patches/postcss-selector-parser@6.0.16.patch",
			"postcss@8.4.38": "patches/postcss@8.4.38.patch",
			"styled-components@6.1.8": "patches/styled-components@6.1.8.patch",
			"click-to-react-component@1.1.0": "patches/click-to-react-component@1.1.0.patch"
		},
		"allowNonAppliedPatches": false
	},
	"config": {
		"commitizen": {
			"path": "./OtomadHelper/Web/node_modules/cz-conventional-changelog-zh",
			"disableScopeLowerCase": false,
			"disableSubjectLowerCase": false,
			"maxHeaderWidth": 100000,
			"maxLineWidth": 100000,
			"defaultType": "",
			"defaultScope": "",
			"defaultSubject": "",
			"defaultBody": "",
			"defaultIssues": "",
			"types": {
				"feat": {
					"description": "一个新功能",
					"title": "Features"
				},
				"fix": {
					"description": "一个bug",
					"title": "Bug Fixes"
				},
				"docs": {
					"description": "文档增删改",
					"title": "Documentation"
				},
				"style": {
					"description": "样式修改(空白、格式、缺少分号等)",
					"title": "Styles"
				},
				"refactor": {
					"description": "既不修复bug也不添加新功能的更改",
					"title": "Code Refactoring"
				},
				"perf": {
					"description": "性能优化",
					"title": "Performance Improvements"
				},
				"test": {
					"description": "增加测试",
					"title": "Tests"
				},
				"build": {
					"description": "影响构建系统或外部依赖项的更改(示例范围:gulp、broccoli、npm)",
					"title": "Builds"
				},
				"ci": {
					"description": "对CI配置文件和脚本的更改(示例范围:Travis, Circle, BrowserStack, SauceLabs)",
					"title": "Continuous Integrations"
				},
				"chore": {
					"description": "除src目录或测试文件以外的修改",
					"title": "Chores"
				},
				"revert": {
					"description": "回退历史版本",
					"title": "Reverts"
				},
				"conflict": {
					"description": "修改冲突",
					"title": "Conflict"
				},
				"font": {
					"description": "字体文件更新",
					"title": "Fonts"
				},
				"delete": {
					"description": "删除文件",
					"title": "Delete Files"
				},
				"stash": {
					"description": "暂存文件",
					"title": "Stash Files"
				}
			}
		}
	}
}
