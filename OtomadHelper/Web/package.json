{
	"name": "otomad-helper",
	"version": "7.0",
	"description": "The front-end part of Otomad Helper script.",
	"type": "module",
	"main": "./dist/index.html",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"release": "npm run build && gh release create $(node -pe \"require('./package.json').version\") $(node -pe \"require('./package.json').main\")",
		"postinstall": "patch-package"
	},
	"author": "Ranne",
	"license": "GPL",
	"dependencies": {
		"@reduxjs/toolkit": "^1.9.7",
		"classnames": "^2.3.2",
		"i18next": "^23.6.0",
		"i18next-browser-languagedetector": "^7.1.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-i18next": "^13.3.1",
		"react-transition-group": "^4.4.5",
		"redux": "^4.2.1",
		"styled-components": "^6.1.0"
	},
	"devDependencies": {
		"@styled/typescript-styled-plugin": "^1.0.0",
		"@types/react": "^18.2.15",
		"@types/react-dom": "^18.2.7",
		"@types/react-transition-group": "^4.4.5",
		"@typescript-eslint/eslint-plugin": "^6.8.0",
		"@typescript-eslint/parser": "^6.8.0",
		"@vitejs/plugin-react": "^4.1.0",
		"@vitejs/plugin-react-swc": "^3.3.2",
		"babel-plugin-styled-components": "^2.1.4",
		"eslint": "^8.45.0",
		"eslint-plugin-react": "^7.31.10",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.3",
		"patch-package": "^8.0.0",
		"typescript": "^5.1.6",
		"unplugin-auto-import": "^0.16.6",
		"vite": "^4.4.5",
		"vite-plugin-svg-icons": "^2.0.1",
		"vite-plugin-svgr": "^4.1.0",
		"vite-tsconfig-paths": "^4.2.1"
	}
}
