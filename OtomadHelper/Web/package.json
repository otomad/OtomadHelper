{
	"name": "Otomad Helper",
	"version": "7.0",
	"description": "The front-end part of Otomad Helper script.",
	"type": "module",
	"main": "./dist/index.html",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"release": "npm run build && gh release create $(node -pe \"require('./package.json').version\") $(node -pe \"require('./package.json').main\")"
	},
	"author": "Ranne",
	"license": "GPL",
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-transition-group": "^4.4.5",
		"styled-components": "^6.1.0"
	},
	"devDependencies": {
		"@styled/typescript-styled-plugin": "^1.0.0",
		"@types/react": "^18.2.15",
		"@types/react-dom": "^18.2.7",
		"@types/react-transition-group": "^4.4.5",
		"@typescript-eslint/eslint-plugin": "^6.8.0",
		"@typescript-eslint/parser": "^6.8.0",
		"@vitejs/plugin-react-swc": "^3.3.2",
		"babel-plugin-styled-components": "^2.1.4",
		"csinterface-ts": "^1.0.3",
		"eslint": "^8.45.0",
		"eslint-plugin-react": "^7.31.10",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.3",
		"typescript": "^5.1.6",
		"unplugin-auto-import": "^0.16.6",
		"vite": "^4.4.5",
		"vite-plugin-babel": "^1.1.3"
	}
}
