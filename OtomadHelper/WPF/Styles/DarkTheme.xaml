<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Color x:Key="ForegroundColor">White</Color>
	<SolidColorBrush x:Key="ForegroundBrush" Color="{DynamicResource ForegroundColor}" />
	<SolidColorBrush x:Key="CardStroke" Color="#19000000" />
	<SolidColorBrush x:Key="SurfaceOverlayBackground" Color="#0D000000" />
	<SolidColorBrush x:Key="ControlBackground" Color="#0FFFFFFF" />
	<SolidColorBrush x:Key="ControlBackgroundSecondary" Color="#15FFFFFF" />
	<SolidColorBrush x:Key="ControlBackgroundTertiary" Color="#08FFFFFF" />
	<SolidColorBrush x:Key="ControlStroke" Color="#11FFFFFF" />
	<LinearGradientBrush x:Key="ControlElevation" StartPoint="0.5,0" EndPoint="0.5,1">
		<GradientStop Color="#17FFFFFF" Offset="0"/>
		<GradientStop Color="#11FFFFFF" Offset="0.1"/>
	</LinearGradientBrush>
</ResourceDictionary>
