<Page
	x:Class="OtomadHelper.UI.Pages.HomePage"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:local="using:OtomadHelper.UI"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d">
	<StackPanel Spacing="8">
		<TextBlock x:Name="ReceivedLbl" Text="Loaded!" TextWrapping="Wrap" />
		<TextBox x:Name="SendTxt" />
		<Button x:Name="SendBtn" Content="Send" Click="SendBtn_Click" AccessKey="S" />
	</StackPanel>
</Page>
